@model Wtm.QuickStart.ViewModel.SchoolVMs.SchoolImportVM

<wt:form vm="@Model">
    <wt:row align="AlignEnum.Right">
        <wt:downloadTemplateButton vm="@Model" />
    </wt:row>
    <wt:upload field="UploadFileId" label-text="上传模板" upload-type="ExcelFile" />
    <wt:grid vm="ErrorListVM" use-local-data="true" />
    <wt:row align=" AlignEnum.Right">
        <wt:submitbutton />
        <wt:closebutton />
    </wt:row>
</wt:form>