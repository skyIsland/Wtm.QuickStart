@model IsMatch.LifeManager.ViewModel.CreditCardVMs.CreditCardVM

<wt:form vm="@Model">
	<wt:quote>确定要删除这条数据么：</wt:quote>
<wt:row items-per-row="ItemsPerRowEnum.Two">
<wt:display field="Entity.ReturnDate" />
<wt:display field="Entity.BillDate" />
<wt:display field="Entity.CreditCardType" />
<wt:display field="Entity.MoneyType" />
<wt:display field="Entity.Money" />
</wt:row>
    <wt:hidden field="Entity.ID" />
    <wt:row align=" AlignEnum.Right">
        <wt:submitbutton theme=" ButtonThemeEnum.Warm" text="删除"/>
        <wt:closebutton />
    </wt:row>
</wt:form>